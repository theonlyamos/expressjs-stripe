extends layout

block content
  .globalContent
  main
    .stripes
      .stripe.s1
      .stripe.s2
      .stripe.s3
    section.container-lg
      // Intro
      //
        .cell.intro
          ul.optionList(role='tablist')
            li(role='tab')
              a(data-lang='de')
                | Deutsch
            li(role='tab')
              a(data-lang='en')
                | English
            li(role='tab')
              a(data-lang='es')
                | Espa&ntilde;ol
            li(role='tab')
              a(data-lang='fr')
                | Fran&ccedil;ais
            li(role='tab')
              a(data-lang='it')
                | Italiano
            li(role='tab')
              a(data-lang='ja')
                | &#x65E5;&#x672C;&#x8A9E;
          h1.common-SuperTitle(data-tid='elements_examples.intro.title') Stripe Elements examples
          p.common-IntroText(data-tid='elements_examples.intro.subtitle')
            | Stripe Elements are pre-built rich UI components that help you build your own pixel-perfect checkout flows across desktop and&nbsp;mobile.
          p.common-ButtonGroup
            a.common-Button.common-Button--default(data-tid='elements_examples.intro.button_learn_more' href='https://stripe.com/elements')
              | Learn more
            a.common-Button(data-tid='elements_examples.intro.button_docs' href='https://stripe.com/docs/stripe-js')
              | Explore the docs
      
      // Example 1
      #example-1.cell.example.example1
        form
          fieldset
            .row
              label(for='example1-name' data-tid='elements_examples.form.name_label') Name
              input#example1-name(data-tid='elements_examples.form.name_placeholder' type='text' placeholder='Jane Doe' required='' autocomplete='name')
            .row
              label(for='example1-email' data-tid='elements_examples.form.email_label') Email
              input#example1-email(data-tid='elements_examples.form.email_placeholder' type='email' placeholder='janedoe@gmail.com' required='' autocomplete='email')
            .row
              label(for='example1-phone' data-tid='elements_examples.form.phone_label') Phone
              input#example1-phone(data-tid='elements_examples.form.phone_placeholder' type='tel' placeholder='(941) 555-0123' required='' autocomplete='tel')
          fieldset
            .row
              #example1-card
          button(type='submit' data-tid='elements_examples.form.pay_button') Pay $25
